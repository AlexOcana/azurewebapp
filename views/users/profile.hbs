<body>
    <div class="container">
        <h1 class="username">{{loggedUser.username}} as {{loggedUser.nickname}}</h1>
        <hr>
        <p class="email">Email: {{loggedUser.email}}</p>
        <hr>
        <p class="description">Description: {{loggedUser.description}}</p>
        <hr>

        <a href="auth/logout" class="btn btn-danger">Log Out</a>
        <a href="/" class="btn btn-dark">Return Home</a>
    </div>
</body>